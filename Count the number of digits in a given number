import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        long num = sc.nextLong();  
        long count = 0;

        while (num > 0) {
            count++; 
            num /= 10;                   
        }

        System.out.println(count);  
        sc.close();
    }
}
